footer.page-footer
	.container
		nav.footer-nav
			.footer-nav__column
				h3.footer-nav__title About
				ul.footer-nav__list
					each item in ['About Us', 'Privacy Policy', 'Terms of Use', 'News', 'Events']
						li.footer-nav__item
							a.footer-nav__link(href="#")= item

			.footer-nav__column
				h3.footer-nav__title Involved
				ul.footer-nav__list 
					each item in ['Work with Us', 'Subscribe for', 'Newsletter', 'Forum']
						li.footer-nav__item
							a.footer-nav__link(href="#")= item

			.footer-nav__column
				h3.footer-nav__title Help Center
				ul.footer-nav__list 
					each item in ['FAQs', 'Contact Us', 'How to Start a', 'Challenge']
						li.footer-nav__item
							a.footer-nav__link(href="#")= item

			.footer-nav__column.socials
				h3.footer-nav__title Connect with Us
				ul.socials__list 
					each item in ['fb', 'tw', 'goo']
						li.socials__item
							a(href="#" class="socials__link socials__link--" + item)
								img.socials__img(src="images/icons/png/"+ item + "-ico.png", alt= item)
	.copy
		.container
			.cpoy__text Â© 2015  All Rights Reserved

